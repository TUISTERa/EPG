<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="UseJsonConfig" value="true" /> 
    <add key="ConfigDir" value="$ConfigDir" />
    <add key="GrabingTempFolder" value="$GrabbingTempFolder" />
    <add key="WebGrabFolder" value="" />
    <add key="ProcessTimeout" value="40" />
    <add key="ConvertTimesToLocal" value="true" />
    <add key="MaxAsyncProcesses" value="$MaxAsyncProcesses" />
    <add key="RandomStartOrder" value="false" />
    <add key="ShowWebGrabConsole" value="false" />
    <add key="RemoveChannelsWithNoProgrammes" value="true" />
    <add key="CopyOnlyTitleForOffsetChannel" value="true" />
    <add key="RemoveExtraChannelAttributes" value="true" />
    <add key="GenerateResultsReport" value="true" />
    <add key="ReportFileName" value="report.html" />
    <add key="ReportFolder" value="$ReportFolder" />
  </appSettings>
</configuration>
